{
  "name": "project-dashboard",
  "version": "0.0.1",
  "description": "Simple application to track status and budget for projects integrating with Harvest, Forecast, and Asana.",
  "main": "src/server.js",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run package\" \"npm run server\" ",
    "server": "node src/server.js",
    "postinstall": "typings install",
    "tsc": "tsc",
    "package": "gulp package",
    "tsc:w": "tsc -w",
    "typings": "typings"
  },
  "keywords": [
    "Harvest",
    "Forecast",
    "Asana"
  ],
  "author": "Pico Logic LLC",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.1",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.4",
    "angular2-in-memory-web-api": "0.0.14",
    "asana": "^0.14.1",
    "bluebird": "^3.4.1",
    "bootstrap": "^3.3.7",
    "config": "^1.21.0",
    "core-js": "^2.4.0",
    "express": "^4.14.0",
    "harvest": "^0.1.5",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "underscore": "^1.8.3",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "del": "^2.2.1",
    "gulp": "^3.9.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-typescript": "^2.13.6",
    "gulp-watch": "^4.3.9",
    "jshint": "^2.9.2",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  }
}
